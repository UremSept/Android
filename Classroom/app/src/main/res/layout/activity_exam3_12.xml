<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.houjie.learn.exam3_12">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/exam3_12_image1"
        android:src="@mipmap/a1"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/exam3_12_image2"
        android:src="@mipmap/ic_launcher"
        android:layout_toRightOf="@id/exam3_12_image1"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="湖南理工学院"
        android:id="@+id/exam3_12_text1"
        android:layout_toRightOf="@id/exam3_12_image1"
        android:layout_below="@id/exam3_12_image2"



        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="http://www.hnist.cn"
        android:id="@+id/exam3_12_button1"
       android:layout_below="@id/exam3_12_text1"

        />
</RelativeLayout>
